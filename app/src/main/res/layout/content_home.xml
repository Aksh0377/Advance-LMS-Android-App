<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/demo3"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.axay.o2hleave.HomeActivity"
    tools:showIn="@layout/activity_home">

    <Button
        android:id="@+id/home_register"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="@color/o2hOne"
        android:layout_weight="1"
        android:text="SignUp"
        android:textColor="#ffffff"/>

    <Button
        android:id="@+id/home_login"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@color/o2hSecond"
        android:layout_weight="1"
        android:text="SignIn"
        android:textColor="#ffffff"
        android:layout_toLeftOf="@+id/home_register"
        android:layout_toStartOf="@+id/home_register" />


</RelativeLayout>
